{
  "name": "local-movie-player",
  "version": "1.0.0",
  "description": "",
  "main": "dist/js/main.js",
  "scripts": {
    "copy": "cp src/index.html dist/index.html",
    "build": "npm run copy && webpack --mode production",
    "watch": "npm run copy && webpack --mode production --watch",
    "watch:bg": "npm run watch &",
    "ui": "webpack-dev-server --content-base dist --mode production --inline",
    "ui:bg": "npm run ui &",
    "api": "node src/js/api.js",
    "api:bg": "npm run api &",
    "killall": "killall node",
    "start": "npm run watch:bg && npm run ui:bg && npm run api:bg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otchy210/local-movie-player.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/otchy210/local-movie-player/issues"
  },
  "homepage": "https://github.com/otchy210/local-movie-player#readme",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
